const arraydecosas=[]

function agregarvalor(valor){
    #input del texto pusheado a un array#
    arraydecosas.push(valor)

}

indexof()#esta funcion te devuelve el valor de la variable que queres
function encontrarelemento(elementos){
    for (let i = 0, i < arraydecosas.lenght , i++){
        if (arraydecosas[i]== elementosaencontrar){
            return i
        }
    }
}

function invertir(arrayinvertir){
    let arrayinvertido = []
    for (let i = 0, i <= arrayinvertir.lenght , i++){
        let pivot = arrayinvertir[i]
        arrayinvertido.push(pivot)
    }

}


